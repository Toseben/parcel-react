{
  "name": "parcel-reactjs",
  "version": "1.0.0",
  "description": "A minimal React.js boilerplate with parcel as bundler",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev": "parcel ./src/index.html",
    "build": "parcel build ./src/index.html/"
  },
  "dependencies": {
    "autoprefixer": "^9.3.1",
    "react": "^16.6.0",
    "react-dom": "^16.6.0"
  },
  "devDependencies": {
    "node-sass": "^4.9.4",
    "parcel-bundler": "^1.10.3"
  },
  "postcss": {
    "modules": false,
    "plugins": {
      "autoprefixer": {
        "browsers": [
          ">1%",
          "last 4 versions",
          "Firefox ESR",
          "not ie < 9"
        ],
        "flexbox": "no-2009"
      }
    }
  }
}
